				</page>
				<page id="trash">
					<pageTitle>Trash</pageTitle>
					<new>New</new>
					<inbox>Inbox</inbox>
					<alerts>Alerts</alerts>
					<archive>Archive</archive>
					<trash>Trash</trash>
					<outbox>Outbox</outbox>
					<restore>Restore</restore>
					<delete>Delete</delete>
					<deletePermanently>Delete</deletePermanently>
					<trash>Trash</trash>
					<from>From</from>
					<subject>Subject</subject>
