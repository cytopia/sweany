					<moveToTrash>Trash</moveToTrash>
					<from>From</from>
					<to>To</to>
					<subject>Subject</subject>
					<received>Received</received>
					<sent>Sent</sent>
				</page>
				<page id="reply">
					<pageTitle>Relply</pageTitle>
					<new>New</new>
					<inbox>Inbox</inbox>
