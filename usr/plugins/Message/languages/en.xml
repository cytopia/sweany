					<errFillOutBody>Write some text.</errFillOutBody>
				</page>
				<page id="write">
					<pageTitle>Write</pageTitle>
					<new>New</new>
					<inbox>Inbox</inbox>
					<alerts>Alerts</alerts>
					<archive>Archive</archive>
					<trash>Trash</trash>
					<outbox>Outbox</outbox>
					<to>To</to>
					<username>Username</username>
					<subject>Subject</subject>
					<send>Send</send>
					<errUserNameDoesNotExist>This user does not exist!</errUserNameDoesNotExist>
					<errFillOutSubject>Write a subject.</errFillOutSubject>
					<errFillOutBody>Write some text.</errFillOutBody>
				</page>
			</PageSection>

			<BlockSection>
				<block id="getMyNewMessageCountLink">
					<noNewMessages>no new Messages</noNewMessages>
					<aNewMessage>new Message</aNewMessage>
					<NewMessages>new Messages</NewMessages>
					<aNewAlert>new Alert</aNewAlert>
					<NewAlerts>new Alerts</NewAlerts>
				</block>
			</BlockSection>
		</Message>
	</plugins>
</root>
