					<moveToArchive>Archive</moveToArchive>
					<from>From</from>
					<subject>Subject</subject>
					<received>Received</received>
					<newMessage>new Message</newMessage>
					<readMessage>read Message</readMessage>
					<repliedMessage>replied Message</repliedMessage>
				</page>
				<page id="outbox">
					<pageTitle>Sent Nachrichten</pageTitle>
					<new>New</new>
					<inbox>Inbox</inbox>
					<alerts>Alerts</alerts>
					<archive>Archive</archive>
					<trash>Trash</trash>
					<outbox>Outbox</outbox>
					<delete>Delete</delete>
					<to>To</to>
					<subject>Subject</subject>
					<sent>Sent</sent>
					<newMessage>new Message</newMessage>
					<readMessage>read Message</readMessage>
					<repliedMessage>replied Message</repliedMessage>
				</page>
				<page id="alerts">
					<pageTitle>Alerts</pageTitle>
					<new>New</new>
					<inbox>Inbox</inbox>
					<alerts>Alerts</alerts>
					<archive>Archive</archive>
					<trash>Trash</trash>
					<outbox>Outbox</outbox>
					<moveToTrash>Trash</moveToTrash>
					<moveToArchive>Archive</moveToArchive>
					<from>From</from>
					<subject>Subject</subject>
					<received>Received</received>
					<newAlert>new Alert</newAlert>
					<readAlert>read Alert</readAlert>
				</page>
				<page id="archive">
					<pageTitle>Archive</pageTitle>
					<new>New</new>
					<inbox>Inbox</inbox>
					<alerts>Alerts</alerts>
					<archive>Archive</archive>
					<trash>Trash</trash>
					<outbox>Outbox</outbox>
